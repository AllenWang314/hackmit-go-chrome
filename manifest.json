{
  "name": "MIT Poker Club P",
  "description": "To activate, type 'p' followed by a tab or space into the Omnibox. Then enter your term and press enter to submit.",
  "version": "1.1.0",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "lib/findAndReplaceDOMText.js",
        "content.js"
      ]
    }
  ],
  "permissions": [
    "<all_urls>"
  ],
  "omnibox": { "keyword" : "p" },
  "manifest_version": 2
}
